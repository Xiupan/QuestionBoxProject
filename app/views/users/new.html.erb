<h1>Register a New User</h1>

<%= form_for @user do |f| %>
  <div class="">
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>

  <div class="">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div class="">
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  </div>

  <div class="">
    <%= f.submit %>
  </div>
<% end %>
